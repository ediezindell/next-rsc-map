{
  "name": "next-rsc-map",
  "version": "0.2.1",
  "description": "A CLI tool to analyze Next.js projects and map out Server/Client components.",
  "main": "dist/index.js",
  "bin": {
    "next-rsc-map": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
