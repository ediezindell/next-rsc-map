    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ediezindell/next-rsc-map.git"
  },
  "keywords": [
    "next",
    "nextjs",
    "rsc",
    "server components",
    "client components",
    "cli",
    "analyzer",
    "map"
  ],
  "author": "ediezindell",
  "license": "MIT",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/ediezindell/next-rsc-map/issues"
  },
  "homepage": "https://github.com/ediezindell/next-rsc-map#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^24.7.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@types/yargs": "^17.0.33",
    "chalk": "^5.6.2",
    "fast-glob": "^3.3.2",
